{
  "name": "netlify-plugin-commercetools-frontend",
  "version": "0.0.1",
  "description": "Netlify Build plugin - Commercetools Frontend build extension",
  "type": "module",
  "exports": "./src/index.js",
  "main": "./src/index.js",
  "files": [
    "src/**/*.js",
    "src/**/*.json",
    "src/**/*.sh",
    "src/**/*.html",
    "src/**/*.ejs",
    "manifest.yml"
  ],
  "keywords": [
    "netlify-plugin",
    "netlify"
  ],
  "author": "commercetools GmbH <tobias.schlitt@commercetools.com>",
  "license": "MIT",
  "repository": "FrontasticGmbH/netlfy-plugin-commercetools-frontend",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "engines": {
    "node": "^14.14.0 || >=16.0.0"
  },
  "ava": {
    "verbose": true,
    "timeout": "100s",
    "environmentVariables": {
      "FORCE_COLOR": "1"
    }
  },
  "scripts": {
    "build": "netlify-build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --ignore-path .gitignore --fix --cache --format=codeframe --max-warnings=0 \"{src,init}/**/*.{cjs,mjs,js}\"",
    "prettier": "prettier --ignore-path .gitignore --write --loglevel warn \"{.github,src,init}/**/*.{cjs,mjs,js,md,yml,json}\" \"*.{cjs,mjs,js,md,yml,json}\"",
    "ava": "cross-env FORCE_COLOR=1 ava --verbose",
    "release": "release-it"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.1",
    "vitest-fetch-mock": "^0.2.2"
  },
  "devDependencies": {
    "@netlify/build": "^26.5.3",
    "ava": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^6.0.0",
    "netlify-cli": "^9.16.2",
    "prettier": "^2.1.2",
    "release-it": "^15.0.0",
    "vitest": "^0.29.7"
  }
}
